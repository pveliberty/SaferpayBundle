{
    "name": "payment/saferpay-bundle",
    "type": "symfony-bundle",
    "description": "Saferpay payment service library for symfony",
    "keywords": ["saferpay", "payment", "e-commerce", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Meier",
            "email": "sk.mykon@gmail.com"
        },
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "payment/saferpay": "master",
        "symfony/config": ">=2.1.0,<3.0",
        "symfony/dependency-injection": ">=2.1.0,<3.0",
        "symfony/http-kernel": ">=2.1.0,<3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "repositories": [
        {
          "type": "package",
          "package": {
          "version": "master",
          "name": "payment/saferpay",
          "source": {
            "url": "https://github.com/pvesin/saferpay.git",
                   "type": "git",
                   "reference": "master"
           }
         }
        }
    ],
    "autoload": {
        "psr-0": { "Payment\\Bundle\\SaferpayBundle\\": "" }
    },
    "target-dir": "Payment/Bundle/SaferpayBundle"
}